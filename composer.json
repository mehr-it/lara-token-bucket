{
  "name": "mehr-it/lara-token-bucket",
  "description": "Implements the token bucket algorithm using laravel cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\LaraTokenBucket\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MehrItLaraTokenBucketTest\\": "test/"
    }
  },
  "require": {
    "php": ">=7.3.0",
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4|^8.4|^9.5",
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MehrIt\\LaraTokenBucket\\Provider\\TokenBucketServiceProvider"
      ],
      "aliases": {
        "Assets": "MehrIt\\LaraTokenBucket\\Facades\\TokenBucket"
      }
    }
  }
}
